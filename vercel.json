{
  "name": "nuxt-buyfast",
  "version": 2,
  "framework": "nuxtjs",
  "outputDirectory": ".output",
  "public": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate=59"
        }
      ]
    }
  ]
}
