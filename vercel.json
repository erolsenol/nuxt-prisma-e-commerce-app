{
  "name": "nuxt-buyfast",
  "version": 2,
  "framework": "nuxtjs",
  "outputDirectory": ".output",
  "public": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
