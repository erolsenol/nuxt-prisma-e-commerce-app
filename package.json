{
  "name": "nuxt-app",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "prisma generate && prisma migrate status --schema=./prisma/schema.prisma && nuxt build",
    "build:nitro": "prisma generate && nitro build",
    "dev": "nuxt dev",
    "start": "nux start",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "prisma generate && prisma migrate status --schema=./prisma/schema.prisma",
    "migrate": "npx prisma migrate dev",
    "migrate reset": "npx prisma migrate reset",
    "deploy": "/opt/cpanel/ea-nodejs10/bin/pm2 start /opt/cpanel/ea-nodejs10/bin/npm --name yourpm2name -- start",
    "seed": "node --loader ts-node/esm prisma/seed.ts",
    "seedd": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "devDependencies": {
    "@hypernym/nuxt-anime": "^2.1.0",
